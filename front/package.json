{
  "name": "slotfinder-front",
  "version": "0.1.0",
  "license": "MIT",
  "description": "SlotFinder front-end",
  "type": "module",
  "scripts": {
    "start": "rsbuild dev",
    "start:mock": "rsbuild dev --env-mode mock",
    "start:storybook": "storybook dev -p 3002",
    "build": "rsbuild build",
    "build:storybook": "storybook build",
    "preview": "rsbuild preview",
    "test": "node scripts/test.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:browser": "vitest --config=vitest.browser.config.ts --run",
    "test:browser:update": "vitest --config=vitest.browser.config.ts --run --update",
    "test:browser:watch": "vitest --config=vitest.browser.config.ts",
    "lint": "node scripts/lint.js",
    "lint:oxlint": "oxlint src/ mock/",
    "lint:oxlint:fix": "oxlint --fix src/ mock/",
    "lint:stylelint": "stylelint \"src/**/*.{scss,css}\"",
    "lint:stylelint:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "lint:prettier": "prettier \"{mocks,public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --check --ignore-unknown",
    "lint:prettier:fix": "prettier \"{mocks,public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --write --ignore-unknown",
    "lint:tsc": "tsc --noEmit",
    "package:check": "npm exec --yes package-lock-utd@1.1.3",
    "postinstall": "msw init",
    "i18next-resources-for-ts": "i18next-resources-for-ts toc -i ./src/i18n/locales/en -o ./src/i18n/@types/resources.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@material-symbols/svg-400": "^0.40.2",
    "@tanstack/react-query": "^5.90.20",
    "i18next": "^25.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.4",
    "react-router": "^7.13.0",
    "stylelint-selector-bem-pattern": "^4.0.1",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@pplancq/postcss-config": "*",
    "@pplancq/prettier-config": "*",
    "@pplancq/stylelint-config": "*",
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-react": "^1.4.5",
    "@rsbuild/plugin-sass": "^1.5.0",
    "@rsbuild/plugin-svgr": "^1.3.0",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-onboarding": "^10.2.0",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.2.1",
    "i18next-resources-for-ts": "^2.0.0",
    "jsdom": "^27.4.0",
    "monocart-coverage-reports": "^2.12.9",
    "msw": "^2.12.8",
    "oxlint": "^1.43.0",
    "prettier": "^3.8.1",
    "prettier-plugin-sh": "^0.18.0",
    "remark-gfm": "^4.0.1",
    "storybook": "^10.2.0",
    "storybook-react-rsbuild": "^3.2.2",
    "stylelint": "^16.26.1",
    "stylelint-prettier": "^5.0.3",
    "stylelint-webpack-plugin": "^5.1.0",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.3",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18",
    "vitest-axe": "^1.0.0-pre.5",
    "vitest-browser-react": "^2.0.5",
    "vitest-sonar-reporter": "^3.0.0"
  },
  "volta": {
    "node": "24.13.0",
    "npm": "11.8.0"
  },
  "msw": {
    "workerDirectory": [
      "./public"
    ]
  }
}
