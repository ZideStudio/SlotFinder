{
  "name": "slotfinder-front",
  "version": "0.1.0",
  "license": "MIT",
  "description": "SlotFinder front-end",
  "type": "module",
  "scripts": {
    "start": "rsbuild dev",
    "start:mock": "rsbuild dev --env-mode mock",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "test": "node scripts/test.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "lint": "node scripts/lint.js",
    "lint:oxlint": "oxlint src/ mock/",
    "lint:oxlint:fix": "oxlint --fix src/ mock/",
    "lint:stylelint": "stylelint \"src/**/*.{scss,css}\"",
    "lint:stylelint:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "lint:prettier": "prettier \"{mocks,public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --check --ignore-unknown",
    "lint:prettier:fix": "prettier \"{mocks,public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --write --ignore-unknown",
    "lint:tsc": "tsc --noEmit",
    "package:check": "npm exec --yes package-lock-utd@1.1.3",
    "prepare": "cd .. && husky front/.husky",
    "postinstall": "msw init"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-router": "^7.8.2",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@pplancq/postcss-config": "*",
    "@pplancq/prettier-config": "*",
    "@pplancq/stylelint-config": "*",
    "@rsbuild/core": "^1.5.10",
    "@rsbuild/plugin-react": "^1.4.0",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "monocart-coverage-reports": "^2.12.8",
    "msw": "^2.11.2",
    "nodemon": "^3.1.10",
    "oxlint": "^1.16.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-webpack-plugin": "^5.0.1",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.2",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-axe": "^1.0.0-pre.5",
    "vitest-sonar-reporter": "^2.0.4"
  },
  "volta": {
    "node": "22.19.0",
    "npm": "11.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": [
      "./public"
    ]
  }
}
