{
  "name": "slotfinder-front",
  "version": "0.1.0",
  "license": "MIT",
  "description": "SlotFinder front-end",
  "type": "module",
  "scripts": {
    "start": "rsbuild dev",
    "start:mock": "rsbuild dev --env-mode mock",
    "build": "rsbuild build",
    "preview": "rsbuild preview",
    "test": "node scripts/test.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "lint": "node scripts/lint.js",
    "lint:oxlint": "oxlint src/ mock/",
    "lint:oxlint:fix": "oxlint --fix src/ mock/",
    "lint:stylelint": "stylelint \"src/**/*.{scss,css}\"",
    "lint:stylelint:fix": "stylelint \"src/**/*.{scss,css}\" --fix",
    "lint:prettier": "prettier \"{mocks,public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --check --ignore-unknown",
    "lint:prettier:fix": "prettier \"{mocks,public,src}/**/*.!(js|jsx|mjs|ts|tsx|scss|css)\" --write --ignore-unknown",
    "lint:tsc": "tsc --noEmit",
    "package:check": "npm exec --yes package-lock-utd@1.1.3",
    "prepare": "cd .. && husky front/.husky",
    "postinstall": "msw init",
    "i18next-resources-for-ts": "i18next-resources-for-ts toc -i ./src/i18n/locales/en -o ./src/i18n/@types/resources.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "i18next": "^25.5.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^7.9.3",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@pplancq/postcss-config": "*",
    "@pplancq/prettier-config": "*",
    "@pplancq/stylelint-config": "*",
    "@rsbuild/core": "^1.5.13",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rsbuild/plugin-svgr": "^1.2.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "i18next-resources-for-ts": "^1.7.4",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "monocart-coverage-reports": "^2.12.9",
    "msw": "^2.11.3",
    "nodemon": "^3.1.10",
    "oxlint": "^1.20.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-webpack-plugin": "^5.0.1",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.3",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-axe": "^1.0.0-pre.5",
    "vitest-sonar-reporter": "^2.0.4"
  },
  "volta": {
    "node": "22.20.0",
    "npm": "11.6.1"
  },
  "msw": {
    "workerDirectory": [
      "./public"
    ]
  }
}
